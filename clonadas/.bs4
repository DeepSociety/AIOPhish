#!/usr/bin/python

import requests, sys
from bs4 import BeautifulSoup
URL = sys.argv[1]

try:
 content = requests.get(URL)
 soup = BeautifulSoup(content.text, 'html.parser')
except:
 print("Error, comprueba tu conexi√≥n a internet")

try:
 inputs=[(n['name']) for n in soup.findAll('input')]
 for inpt in inputs:
  print(inpt)
except:
 print("Error")
